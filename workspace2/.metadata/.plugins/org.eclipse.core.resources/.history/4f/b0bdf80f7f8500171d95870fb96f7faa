package fibo;

import java.util.Scanner;

public class fibon {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=10;
		int row=1;
		int col=1;
		int i;
		while (row<=4){
			col=1;
			while(col<=row)
			{
				for(i=1;i<=10;i++);
				{
					System.out.println(i);
				}
			}col++;
		}row++;
	}
	
	
	
	
}